{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": { "path": "server.py", "entrypoint": "mcp" },
  "environment": { "dependencies": ["fastmcp", "httpx>=0.27"] },
  "deployment": {
    "transport": "http",
    "path": "/mcp/",
    "env": {
      "TAVILY_API_KEY": "${TAVILY_API_KEY}"   // set this in the cloud UI
    }
  }
}
